//Z531415M JOB 01,ROB,MSGCLASS=X,NOTIFY=&SYSUID
//*
//*       INSTALL TSTVS ON AN MVS SYSTEM WITH A
//*       SECURITY SYSTEM INSTALLED LIKE RACF
//*
//ALLOC   PROC OBJ='Z531415.TSTVS.OBJ',
//             LOADLIB='Z531415.TSTVS.LOAD'
//IEFBR14 EXEC PGM=IEFBR14
//OBJ      DD  DSN=&OBJ,UNIT=SYSALLDA,VOL=SER=PUB002,
//             SPACE=(CYL,(1,1,5)),
//             DCB=(RECFM=FB,LRECL=80,BLKSIZE=3200),
//             DISP=(NEW,CATLG)
//LOAD     DD  DSN=&LOADLIB,UNIT=SYSALLDA,VOL=SER=PUB002,
//             SPACE=(CYL,(1,1,5)),
//             DCB=(RECFM=U,BLKSIZE=6144),
//             DISP=(NEW,CATLG)
//        PEND
//*
//ASM     PROC OUT='*',MEMBER=DUMMY,
//             ASMBLR=IFOX00,           * CHANGE TO THE REQUESTED ASM
//             MACLIB1='Z531415.TSTVS.ASM',
//             MACLIB2='SYS1.MACLIB',
//             MACLIB3='SYS1.AMODGEN',
//             ASMPARM='',
//             SRCLIB='Z531415.TSTVS.ASM',
//             OBJ='Z531415.TSTVS.OBJ'
//ASM     EXEC PGM=&ASMBLR,PARM='OBJ,NODECK,LINECOUNT(51)&ASMPARM'
//SYSPRINT DD  SYSOUT=&OUT
//SYSUT1   DD  UNIT=SYSDA,SPACE=(3200,(100,200))
//SYSUT2   DD  UNIT=SYSDA,SPACE=(3200,(100,200))
//SYSUT3   DD  UNIT=SYSDA,SPACE=(3200,(100,200))
//SYSGO    DD  DSN=&OBJ(&MEMBER),DISP=SHR
//SYSLIB   DD  DSN=&MACLIB1,DISP=SHR,DCB=BLKSIZE=32000
//         DD  DSN=&MACLIB2,DISP=SHR
//         DD  DSN=&MACLIB3,DISP=SHR
//SYSIN    DD  DSN=&SRCLIB(&MEMBER),DISP=SHR
//        PEND
//*
//STEP00  EXEC ALLOC
//*
//STEP01  EXEC ASM,MEMBER=TSTVS
//STEP02  EXEC ASM,MEMBER=TSTVSALL,ASMPARM=',SYSPARM(MVS)'
//STEP03  EXEC ASM,MEMBER=TSTVSCOM
//STEP04  EXEC ASM,MEMBER=TSTVSSEC,ASMPARM=',SYSPARM(MVS)'
//*
//LKED    EXEC PGM=IEWL,PARM='XREF,LIST,NCAL'
//SYSPRINT DD  SYSOUT=*
//SYSUT1   DD  UNIT=SYSDA,SPACE=(3200,(100,300))
//SYSLMOD  DD  DSN=Z531415.TSTVS.LOAD,DISP=SHR
//OBJ      DD  DSN=Z531415.TSTVS.OBJ,DISP=SHR
//SYSLIN   DD  DDNAME=SYSIN
//SYSIN DD *
 INCLUDE OBJ(TSTVS,TSTVSCOM,TSTVSALL)
 INCLUDE OBJ(TSTVSSEC)
 SETCODE AC(1)
 ENTRY TSTVS
 NAME TSTVS(R)
//STEPLST EXEC PGM=IEBCOPY
//SYSPRINT DD  SYSOUT=*
//IN       DD  DSN=Z531415.TSTVS.ASM,DISP=SHR
//OUT      DD  DSN=SYS1.PROCLIB,DISP=SHR
//SYSIN    DD  *
 C I=((IN,R)),O=OUT
 S M=EDIT
/*
